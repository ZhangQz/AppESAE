                x:Name="btnTerapiasCognitivo"
                Clicked="btnTerapiasCognitivo_Clicked"
                />
        <Button Text="Neuropsicologia Clínica"
                x:Name="btnNeuropsicologiaCLinica"
                Clicked="btnNeuropsicologiaCLinica_Clicked"
                />
        <Button Text="Intervenção com Doentes de Alzheimer e outras Demências"
                x:Name="btnIntervençãoDoentes"
                Clicked="btnIntervençãoDoentes_Clicked"
                />
	</ContentPage.Content>
    
</ContentPage>
