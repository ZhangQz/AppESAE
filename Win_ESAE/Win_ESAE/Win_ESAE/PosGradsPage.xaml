                x:Name="btnAvaliacaoIntervencao"
                Clicked="btnAvaliacaoIntervencao_Clicked"
                />
        <Button Text="Psicoterapia Breve"
                x:Name="btnPsicoterapiaBreve"
                Clicked="btnPsicoterapiaBreve_Clicked"
                />
        <Button Text="Psicoterapia Psicanalítica"
                x:Name="btnPsicoterapiaPsicanalitica"
                Clicked="btnPsicoterapiaPsicanalitica_Clicked"
                />
        <Button Text="Psicoterapia Da Criança e Do Adolescente"
                x:Name="btnPsicoterapiaCrianca"
                Clicked="btnPsicoterapiaCrianca_Clicked"
                />
